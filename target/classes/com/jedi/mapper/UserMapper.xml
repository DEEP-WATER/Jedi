<?xml version="1.0" encoding="UTF-8" ?>    
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >    
<mapper namespace="com.jedi.dao.IUserDao" >
	
	<insert id="insertUser" parameterType="com.jedi.bean.User" useGeneratedKeys="true" keyProperty="id">
		insert into
		User(id,name)
		values(#{id},#{name})
	</insert>

</mapper>
